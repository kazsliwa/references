	Archiveprefix = {arXiv},
	Author = {{Haan}, S. and {Surace}, J.~A. and {Armus}, L. and {Evans}, A.~S. and {Howell}, J.~H. and {Mazzarella}, J.~M. and {Kim}, D.~C. and {Vavilkin}, T. and {Inami}, H. and {Sanders}, D.~B. and {Petric}, A. and {Bridge}, C.~R. and {Melbourne}, J.~L. and {Charmandaris}, V. and {Diaz-Santos}, T. and {Murphy}, E.~J. and {U}, V. and {Stierwalt}, S. and {Marshall}, J.~A.},
	Date-Added = {2016-11-25 13:25:29 +0000},
