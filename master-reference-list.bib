	Journal = {\apjl},
	Keywords = {Emission Spectra, Galactic Structure, Interplanetary Gas, Mass Distribution, Molecular Gases, Astronomical Models, Carbon Monoxide, Infrared Astronomy, Luminosity},
	Month = sep,
	Pages = {L13-L16},
	Title = {{Molecular gas mass and far-infrared emission from distant luminous galaxies}},
	Volume = 414,
