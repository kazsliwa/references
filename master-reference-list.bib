	Archiveprefix = {arXiv},
	Author = {{Gratier}, P. and {Pety}, J. and {Guzm{\'a}n}, V. and {Gerin}, M. and {Goicoechea}, J.~R. and {Roueff}, E. and {Faure}, A.},
	Date-Added = {2016-07-30 17:03:55 +0000},
	Date-Modified = {2016-07-30 17:04:08 +0000},
	Doi = {10.1051/0004-6361/201321031},
	Eid = {A101},
