	Doi = {10.1086/590484},
	Eid = {957-977},
	Eprint = {0806.0217},
	Journal = {\apj},
	Keywords = {galaxies: evolution, galaxies: individual: Arp 220, galaxies: ISM, galaxies: starburst, quasars: general},
	Month = sep,
