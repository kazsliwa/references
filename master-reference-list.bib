	Date-Modified = {2016-12-08 10:53:18 +0000},
	Doi = {10.1086/522363},
	Eid = {172-193},
	Eprint = {0708.1766},
	Journal = {\apj},
	Keywords = {galaxies: abundances, galaxies: evolution, galaxies: interactions, galaxies: ISM, galaxies: kinematics and dynamics, infrared: galaxies},
